{
  "name": "Image Grabber",
  "description": "Extract all images from current web page",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "70": "icons/70x70.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "activeTab"],
  "background": {}
}
